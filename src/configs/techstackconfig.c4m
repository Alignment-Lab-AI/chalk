tech_stack_rule sqlite_generic {
  regex: "\bsqlite:\/\/\b"
  category: "database"
  subcategory: "sqlite"
  description: "sqlite uris"
}

import_limit := 20

tech_stack_rule mysql_python {
  regex: "^import\s+(mysql.connector|pymysql|MySQLdb)\b"
  category: "database"
  subcategory: "mySQL"
  description: "import of common packages related to mysql"
  file_scope {
    filetypes: ["py"]
    head: 20
  }
}
